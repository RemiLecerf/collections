      <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/">Volcanic Ash RGB - MSG - 0 degree</dc:title>
      <dc:type xmlns:dc="http://purl.org/dc/elements/1.1/">series</dc:type>
      <dc:subject xmlns:dc="http://purl.org/dc/elements/1.1/">Aerosol</dc:subject>
